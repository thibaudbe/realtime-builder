{
  "name": "next-todo-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.1.29",
  "type": "module",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('bun') === -1) throw new Error('Please use Bun as your package manager (bun install)');\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "ws-server": "bun run server/hocuspocus.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
