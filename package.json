    "@tanstack/react-query": "^5.90.5",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "1.0.6",
    "y-websocket": "1.4.6",
    "yjs": "^13.6.27",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "5.0.4",
    "jsdom": "^27.0.0",
